  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.14.1",
    "sequelize": "^6.35.2",
    "sqlite3": "^5.1.6",
    "node-cron": "^3.0.3",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=16.7.0"
  }
}
