{
  "name": "robot-team-bot",
  "version": "1.0.0",
  "description": "Discord bot system with 7 robot personas for scheduled posting",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "deploy": "node deploy-commands.js",
    "db:reset": "node -e \"require('./src/models').sequelize.sync({ force: true }).then(() => process.exit())\"",
    "test": "node test-bot.js"
  },
  "keywords": [
    "discord",
    "bot",
    "scheduling",
    "robots",
    "webhooks"
  ],
  "author": "Robot Team Development",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.19.3",
    "dotenv": "^16.5.0",
    "node-cron": "^3.0.3",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  },
  "engines": {
    "node": ">=16.7.0"
  },
  "directories": {
    "doc": "docs"
  }
}
